{
  "name": "client",
  "version": "1.0.0",
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "coverage": "vitest run --coverage",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage.enabled=true --api 8001",
    "dev": "vite --port 5173 --host",
    "build": "vite build --emptyOutDir --outDir ../backend/public/react",
    "build-dev": "vite build --watch --emptyOutDir --outDir ../backend/public/react",
    "build-local": "vite build --emptyOutDir --outDir ./dist",
    "tsc": "npx tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite build --emptyOutDir --outDir ./dist && vite preview"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@clerk/clerk-react": "^5.8.1",
    "@cloudinary/react": "^1.13.0",
    "@cloudinary/url-gen": "^1.16.1",
    "@heroicons/react": "^2.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.7",
    "daisyui": "^4.7.0",
    "lucide-react": "^0.358.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "react-select": "^5.8.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",
    "vite": "^5.3.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitest/coverage-v8": "^2.0.3",
    "@vitest/ui": "^2.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.1.0",
    "vitest": "^2.0.3"
  }
}
